head
		meta(charset=page.charset)
		meta(http-equiv='X-UA-Compatible', content='IE=edge,chrome=1')
		meta(name='language', content=page.lang)
		title #{page.title}
		meta(name='title', content=page.title)
		meta(name='description', content=page.description)
		meta(name='author', content=page.author)
		meta(name='viewport', content=page.viewport)
		meta(name='robots', content=page.robots)
		meta(name='keywords', content=page.keywords)
		meta(property='og:description', content=page.description)
		meta(property='og:image', content=page.og.image)
		meta(property='og:image:height', content=page.og.image_height)
		meta(property='og:image:type', content=page.og.image_type)
		meta(property='og:image:width', content=page.og.image_width)
		meta(property='og:site_name', content=page.name)
		meta(property='og:title', content=page.title)
		meta(property='og:type', content=page.og.type)
		meta(property='og:url', content=page.url)

		link(href=`${page.static_url}css/layout.css` media='all', rel='stylesheet', type='text/css')
		link(href=`${page.static_url}css/fonts.css` media='all', rel='stylesheet', type='text/css')
		link(href=`${page.static_url}js/libs/datedropper/datedropper.min.css` media='all', rel='stylesheet', type='text/css')
		link(href=`https://fonts.googleapis.com/css?family=Pacifico` media='all', rel='stylesheet', type='text/css')


		link(href=`${page.img_url}favicon.png`, rel='icon', type='image/vnd.microsoft.icon')
		link(href=`${page.img_url}favicon.png`, rel='shortcut icon', type='image/vnd.microsoft.icon')
		link(href=`${page.img_url}favicon.png`, rel='shortcut icon', type='image/x-icon')
		link(href=`${page.og.image}`, rel='image_src', type='image/jpeg')
		+ie('if lte IE 9')
				link(href=`${page.static_url}css/ie.css` media='all', rel='stylesheet', type='text/css')
				script(src=`${page.static_url}js/libs/selectivizr/selectvizr.js`)
				script(src=`${page.static_url}js/libs/html5shiv/dist/html5shiv.js`)
		//-script(src=`${page.static_url}js/libs/modernizr/modernizr.s`)
		include ../_partials/head_scripts
