.icon
	-webkit-font-smoothing antialiased
	display inline-block
	font-family "iconFonts"
	font-size 22px
	font-style normal
	font-variant normal
	font-weight normal
	line-height 1
	text-decoration inherit
	text-transform none
	vertical-align middle


.icon-close:before
	content "\E001"

.icon-delete:before
	content "\E002"

.icon-menu:before
	content "\E003"

.icon-pointer:before
	content "\E004"

.icon-triangle:before
	content "\E005"

.icon-warning:before
	content "\E006"
