- var page = {}
- page.charset         = 'utf-8'
- page.lang            = 'en'
- page.static_url  	   = './arquivos/' // '../../../'
- page.url  	   	   = 'http://localhost:3000'
- page.img_url 		   = page.static_url// + 'img/'
- page.title           = 'company.com'
- page.description     = ''
- page.keywords        = ''
- page.author          = ''
- page.name            = 'company.com'
- page.viewport        = 'width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no'
- page.robots          = 'index,follow'
- page.img             = page.img_url + 'logo.png'
