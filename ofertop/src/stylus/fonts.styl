@import "_00-toolbox/variables"


@font-face
	font-family "iconFonts"
	src url($font_path + "iconFonts/iconFonts-webfont.eot")
	src url($font_path + "iconFonts/iconFonts-webfont.eot?#iefix") format("embedded-opentype"), \
		url($font_path + "iconFonts/iconFonts-webfont.woff") format("woff"), \
		url($font_path + "iconFonts/iconFonts-webfont.ttf") format("truetype"), \
		url($font_path + "iconFonts/iconFonts-webfont.svg#iconFonts") format("svg");
	font-style normal
	font-weight normal

@font-face
	font-family "lato-regular"
	src url($font_path + "lato-regular/lato-regular-webfont.eot")
	src url($font_path + "lato-regular/lato-regular-webfont.eot?#iefix") format("embedded-opentype"), \
		url($font_path + "lato-regular/lato-regular-webfont.woff") format("woff"), \
		url($font_path + "lato-regular/lato-regular-webfont.ttf") format("truetype"), \
		url($font_path + "lato-regular/lato-regular-webfont.svg#lato-regular") format("svg");
	font-style normal
	font-weight normal
 